{
    "name": "MarwaMourad15/laravel-myfatoorah",
    "description": "The MyFatoorah Payment Gateway for Laravel.",
    "version": "1.1.1",
    "type": "library",
    "license": [
        "GPL-3.0-only"
    ],
    "require": {
        "php": ">=7",
        "ext-curl": "*",
        "ext-json": "*",
	    "laravel/framework": "^7",
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "files": ["autoload.php"],
        "psr-4": {
            "MarwaMourad15\\LaravelMyfatoorah\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarwaMourad15\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MarwaMourad15\\LaravelMyfatoorah\\MyFatoorahServiceProvider"
            ]
        }
    },
    "keywords": [
        "MyFatoorah",
        "My Fatoorah",
        "Fatoorah",
        "gateway",
        "payment",
        "Shipping",
        "api",
        "commerce",
        "Library"
    ],
    "homepage": "https://myfatoorah.com/",
    "authors": [
        {
            "name": "Marwa Mourad",
            "email": "marwa.mourad.15@gmail.com"
        }
    ]
}
